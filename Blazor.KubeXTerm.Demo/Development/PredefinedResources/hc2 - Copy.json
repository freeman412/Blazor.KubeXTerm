{
    "apiVersion": "v1",
    "kind": "Pod",
    "metadata": {
        "name": "hashcat3",
        "namespace": "default"
    },
    "spec": {
        "containers": [
            {
                "env": [
                    {
                        "name": "DYNAMIC_USER",
                        "value": "david"
                    }
                ],
                "image": "git.freemancraft.com/david/dynamic_user_hashcat",
                "imagePullPolicy": "Always",
                "name": "hashcat_container",
                "resources": {}
            }
        ],
        "dnsPolicy": "ClusterFirst",
        "enableServiceLinks": true,
        "imagePullSecrets": [
            {
                "name": "git-freemancraft-secret"
            }
        ]
    }
}
